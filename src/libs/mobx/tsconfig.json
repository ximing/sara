{
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "declaration": true,
    "module": "es6",
    "outDir": "lib",
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "noUnusedLocals": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"],
  "files": [
    "src/mobx.ts"
  ]
}
