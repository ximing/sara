{
    "name": "sarax",
    "version": "0.1.0",
    "description": "微信小程序状态管理",
    "author": "ximing",
    "main": "./dist/index.js",
    "scripts": {
        "i": "SASS_BINARY_SITE=https://npm.taobao.org/mirrors/node-sass/ npm install --registry=https://registry.npm.taobao.org",
        "dev": "./node_modules/.bin/webpack-dev-server ",
        "prepublish": "npm run build",
        "builddemo": "webpack --config webpack.build.demo.config.js",
        "buildlib": "webpack --config webpack.build.lib.config.js",
        "builddll": "webpack --config webpack.dll.config.js",
        "build": "rimraf dist && mkdir dist && rsync -va -progress  ./src/* ./dist && babel --presets=es2015,stage-0,react  --plugins=transform-decorators-legacy src --out-dir dist --ignore libs",
        "builddev": "rimraf ./example/sara && mkdir ./example/sara && rsync -va -progress  ./src/* ./example/sara && babel --presets=es2015,stage-0,react  --plugins=transform-decorators-legacy src --ignore libs --out-dir ./example/sara --watch",
        "test": "./node_modules/.bin/jest",
        "test-watch": "./node_modules/.bin/jest --watch"
    },
    "keywords": [],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/ximing/sara"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.4",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-jest": "^21.2.0",
        "babel-loader": "7.1.2",
        "babel-plugin-import": "^1.6.0",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "env-cmd": "^7.0.0",
        "eslint": "^4.8.0",
        "eslint-plugin-react": "^7.4.0",
        "file-loader": "^1.1.4",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.2.1",
        "node-sass": "^4.5.3",
        "postcss": "^6.0.12",
        "postcss-clearfix": "^2.0.1",
        "postcss-loader": "^2.0.6",
        "postcss-position": "^1.0.0",
        "postcss-size": "^2.0.0",
        "prettier": "1.12.1",
        "rabjs": "^3.3.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-hot-loader": "^3.0.0-beta.7",
        "regenerator-runtime": "^0.11.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.9.1"
    },
    "jest": {
        "transform": {},
        "testRegex": "(/__tests__/.*|/(test|spec)/.*).(js)$",
        "moduleFileExtensions": [
            "js",
            "json"
        ]
    },
    "files": [
        "dist",
        "src",
        "example",
        "README.md"
    ],
    "engines": {
        "node": ">= 5.0.0",
        "npm": ">= 3.0.0"
    },
    "dependencies": {
        "invariant": "^2.2.4",
        "lodash.clonedeep": "^4.5.0",
        "lodash.trim": "^4.5.1",
        "mobx": "^4.2.1"
    }
}
